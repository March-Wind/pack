{
    "name": "@marchyang/pack",
    "version": "0.3.1",
    "keywords": [
        "web development",
        "node development",
        "bundling web",
        "bundling node",
        "typescript"
    ],
    "description": "A project that supports local development and bundling features for web and node environments, with TypeScript support. It enables efficient development and deployment of web and node applications using modern development practices.",
    "main": "./dist/index.js",
    "bin": {
        "pack2": "./bin/index.mjs"
    },
    "scripts": {
        "dev": "node --loader ts-node/esm ./src/index.ts",
        "dev2": "ts-node ./src/index.ts",
        "dev:debug": " node --inspect-brk  --loader ts-node/esm ./src/index.ts",
        "build:js": "npm run clean:lib && tsc && tsc-alias",
        "build:prod": "webpack",
        "eslint": "eslint ./src --fix",
        "test": "jest --passWithNoTests"
    },
    "author": "marchyang",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/March-Wind/web-pack.git"
    },
    "type": "module",
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --config .prettierrc --write"
        ],
        "*.+(css|less|scss|sass)": "stylelint --fix"
    },
    "dependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.4",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.21.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-syntax-top-level-await": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.21.4",
        "@babel/preset-env": "^7.21.4",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.4",
        "@commitlint/cli": "^17.6.1",
        "@commitlint/config-conventional": "^17.6.1",
        "@svgr/webpack": "^8.0.1",
        "@types/html-webpack-plugin": "^3.2.6",
        "@types/jest": "^29.5.0",
        "@types/mini-css-extract-plugin": "^2.5.1",
        "@types/node": "^18.15.11",
        "@types/postcss-flexbugs-fixes": "^4.2.1",
        "@types/webpack-dev-server": "^4.7.2",
        "@types/webpack-manifest-plugin": "^3.0.5",
        "@types/webpack-merge": "^5.0.0",
        "@types/webpack-node-externals": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "argv": "^0.0.3",
        "autoprefixer": "^10.4.14",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^9.1.2",
        "core-js": "^3.30.1",
        "crypto": "^1.0.1",
        "css-loader": "^6.7.3",
        "dotenv": "^16.0.3",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "fast-css-loader": "^1.0.2",
        "fast-sass-loader": "^2.0.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.3",
